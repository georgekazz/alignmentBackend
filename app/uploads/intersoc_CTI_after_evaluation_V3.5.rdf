        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#PolicyandAwareness"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A policy outlining rules and technical requirements for the creation, management, protection, and lifecycle of authentication passwords.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#PatchExploitedVulnerabilities -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#PatchExploitedVulnerabilities">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Applying security patches, updates, or configuration changes that eliminate the weaknesses used by an adversary to gain entry or escalate privileges. Ensures that attackers cannot repeat the compromise through the same vector.</skos:definition>
        <skos:prefLabel>PatchExploitedVulnerabilities</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Pharming -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Pharming">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Redirecting users to fraudulent websites by manipulating DNS records, routers, or local host resolution to covertly capture sensitive data.</skos:definition>
        <skos:prefLabel>Pharming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#PluginOrExtensionExploit -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#PluginOrExtensionExploit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Exploitation of browser or application extensions and plug-ins (e.g., Flash, Java, office macros) to bypass security controls or execute malicious actions via embedded content.</skos:definition>
        <skos:prefLabel>PluginOrExtensionExploit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#QuarantineEmail -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#QuarantineEmail">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Segregating suspicious or malicious email messages — including attachments or embedded links — to prevent user access and stop phishing, malware delivery, or social engineering propagation.</skos:definition>
        <skos:prefLabel>QuarantineEmail</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ReConnectToNetwork -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ReConnectToNetwork">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <owl:backwardCompatibleWith>Restoring normal network connectivity for previously isolated or remediated systems once they have been verified as clean, secure, and properly configured. Ensures safe reintegration into the production environment without reintroducing risk.</owl:backwardCompatibleWith>
        <skos:definition>Restoring normal network connectivity for previously isolated or remediated systems once they have been verified as clean, secure, and properly configured. Ensures safe reintegration into the production environment without reintroducing risk.</skos:definition>
        <skos:prefLabel>ReConnectToNetwork</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ReIssueTrustedCredentials -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ReIssueTrustedCredentials">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Issuing new, properly secured credentials — such as passwords, API keys, certificates, or cryptographic tokens — to replace any that may have been revoked, reset, or compromised during the incident, ensuring legitimate user and service access is fully restored.</skos:definition>
        <skos:prefLabel>ReIssueTrustedCredentials</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoteAccessPolicy -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoteAccessPolicy">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#PolicyandAwareness"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A policy that governs how users, devices, and systems are permitted to access internal resources from external or untrusted locations.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Remote_File_Inclusion -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Remote_File_Inclusion">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#PathTraversal"/>
        <skos:definition>Using &apos;http://example.com/index.php?file=XXX.php&apos; to include a remote file as code.</skos:definition>
        <skos:prefLabel>Remote File Inclusion</skos:prefLabel>
    </owl:NamedIndividual>
    


