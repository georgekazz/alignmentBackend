        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Gaining high-level system privileges by exploiting vulnerabilities in operating system kernel components or drivers.</skos:definition>
        <skos:prefLabel>KernelPrivilegeEscalation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#LocalPrivilegeEscalation -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#LocalPrivilegeEscalation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Gaining elevated privileges on a system where the attacker already has some level of local execution, exploiting software bugs or insecure permissions.</skos:definition>
        <skos:prefLabel>LocalPrivilegeEscalation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MalwareScanning -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MalwareScanning">
