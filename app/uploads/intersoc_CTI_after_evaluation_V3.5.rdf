        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#Identity"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A legally established organization operating for public, charitable, educational, humanitarian, or advocacy purposes rather than commercial profit.</skos:definition>
        <skos:prefLabel>NonProfiOrganization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#OAuthPhishing -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#OAuthPhishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
