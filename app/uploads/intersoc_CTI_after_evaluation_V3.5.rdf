        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A DDoS attack launched using a botnet composed primarily of compromised Internet-of-Things devices such as cameras, routers, and sensors. These attacks leverage large numbers of low-security endpoints to generate massive distributed traffic volumes.</skos:definition>
        <skos:prefLabel>IoTBotnetAttack</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#KernelPrivilegeEscalation -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#KernelPrivilegeEscalation">
