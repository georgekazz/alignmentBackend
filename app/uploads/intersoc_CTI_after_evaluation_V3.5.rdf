        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Monitoring network or host traffic and system events using intrusion detection signatures or behavioral models to identify known or suspected malicious activity in real time.</skos:definition>
        <skos:prefLabel>IDSInspection</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#IndividualPerson -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#IndividualPerson">
