    <!-- http://iee.ihu.gr/intersoc#INTERSOC -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/intersoc#INTERSOC">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <dct:created>2025-04-30</dct:created>
