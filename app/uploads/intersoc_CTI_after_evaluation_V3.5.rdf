        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <owl:backwardCompatibleWith>Automatically analyzing files, executables, memory, or scripts using static or dynamic techniques to detect malicious artifacts or suspicious behavior.</owl:backwardCompatibleWith>
        <skos:definition>Automatically analyzing files, executables, memory, or scripts using static or dynamic techniques to detect malicious artifacts or suspicious behavior.</skos:definition>
        <skos:prefLabel>MalwareScanning</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MediaPlayerExploit -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MediaPlayerExploit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Abuse of vulnerabilities in multimedia players or codecs using specially crafted audio/video files to execute attacker-controlled code on playback.</skos:definition>
        <skos:prefLabel>MediaPlayerExploit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForAnomalousAuthentication -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForAnomalousAuthentication">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Tracking authentication patterns for signs of account misuse — e.g., impossible travel, unexpected privilege use, or sudden MFA fatigue events — indicating adversary return.</skos:definition>
        <skos:prefLabel>MonitorForAnomalousAuthentication</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForPersistenceReactivation -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForPersistenceReactivation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Monitoring for re-enabled or hidden persistence mechanisms such as registry autoruns, scheduled tasks, startup scripts, or known persistence techniques reappearing.</skos:definition>
        <skos:prefLabel>MonitorForPersistenceReactivation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForRecreatedBackdoors -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForRecreatedBackdoors">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Identifying attempts to reintroduce unauthorized user accounts, remote access services, or implanted access mechanisms previously removed during eradication.</skos:definition>
        <skos:prefLabel>MonitorForRecreatedBackdoors</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForRepeatedMalwareActivity -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForRepeatedMalwareActivity">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Continuously scanning systems and telemetry for indicators of previously removed malware or similar variants attempting to re-infect the environment.</skos:definition>
        <skos:prefLabel>MonitorForRepeatedMalwareActivity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForReusedCredentials -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MonitorForReusedCredentials">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Detecting the use of credentials (passwords, tokens, API keys) believed to be compromised, including attempts from unusual locations or devices.</skos:definition>
        <skos:prefLabel>MonitorForReusedCredentials</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MultiVectorAttack -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#MultiVectorAttack">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A DDoS campaign that combines multiple attack types — such as volumetric floods, protocol state exhaustion, and application-layer request bursts — in coordinated waves to overwhelm defenses and bypass mitigation strategies.</skos:definition>
        <skos:prefLabel>MultiVectorAttack</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#NonprofitOrganization -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#NonprofitOrganization">
