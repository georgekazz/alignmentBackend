        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#Identity"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A private human individual acting outside an organizational role.</skos:definition>
        <skos:prefLabel>IndividualPerson</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#IoTBotnetAttack -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#IoTBotnetAttack">
