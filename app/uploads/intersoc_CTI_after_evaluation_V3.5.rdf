        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Deleting malicious or compromised container images to prevent reinfection in cloud or containerized environments, including removal from registries and runtime nodes.</skos:definition>
        <skos:prefLabel>RemoveMaliciousContainerImage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveMaliciousDrivers -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveMaliciousDrivers">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Removing harmful or unauthorized kernel-level modules or device drivers introduced by an attacker to bypass protections, escalate privileges, or hide malware.</skos:definition>
        <skos:prefLabel>RemoveMaliciousDrivers</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveMaliciousServices -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveMaliciousServices">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Identifying and disabling attacker-created or modified system services that provide persistence or unauthorized execution capabilities.</skos:definition>
        <skos:prefLabel>RemoveMaliciousServices</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveUnauthorizedUsers -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RemoveUnauthorizedUsers">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Identifying and revoking access for accounts that were fraudulently created or whose access was escalated without authorization. This blocks adversaries from maintaining identity-level footholds.</skos:definition>
        <skos:prefLabel>RemoveUnauthorizedUsers</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RestrictOutboundTraffic -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RestrictOutboundTraffic">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Limiting or blocking outbound network connections from affected systems to prevent adversary command-and-control, data leakage, or access to additional targets.</skos:definition>
        <skos:prefLabel>RestrictOutboundTraffic</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RevokeSessionTokens -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RevokeSessionTokens">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Invalidating active authentication tokens or sessions that may have been stolen, abused, or hijacked by adversaries to immediately cut off unauthorized access to applications or accounts.</skos:definition>
        <skos:prefLabel>RevokeSessionTokens</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RotateCompromisedAPIKeys -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#RotateCompromisedAPIKeys">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Revoking and regenerating any API keys, access tokens, or application credentials that may have been stolen or misused by an attacker, removing their ability to authenticate to impacted services.</skos:definition>
        <skos:prefLabel>RotateCompromisedAPIKeys</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SQLInjectionExploit -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SQLInjectionExploit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Injecting malicious SQL commands into input fields or queries to manipulate databases, exfiltrate data, or gain unauthorized control.</skos:definition>
        <skos:prefLabel>SQLInjectionExploit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ServerMisconfigurationExploit -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ServerMisconfigurationExploit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Exploiting insecure server configurations, such as default credentials, unnecessary open services, weak file permissions, or disabled security controls, to compromise the system.</skos:definition>
        <skos:prefLabel>ServerMisconfigurationExploit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ServiceMisconfigurationEscalation -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ServiceMisconfigurationEscalation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Elevating privileges by abusing insecure service configurations, such as weak permissions, unprotected tokens, or unprivileged write access to service binaries.</skos:definition>
        <skos:prefLabel>ServiceMisconfigurationEscalation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SetuidBinaryAbuse -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SetuidBinaryAbuse">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Taking advantage of Unix/Linux binaries with the setuid bit set to escalate privileges, often by manipulating environment variables or injection points.</skos:definition>
        <skos:prefLabel>SetuidBinaryAbuse</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Smishing -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Smishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Phishing using SMS or mobile messaging platforms to distribute malicious links or trigger user actions leading to compromise.</skos:definition>
        <skos:prefLabel>Smishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SourcePort -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SourcePort">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>The network port number used on the originating system or service when initiating communication.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SubscriptionOrMembershipDataLeak -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SubscriptionOrMembershipDataLeak">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Leakage of customer account membership information such as loyalty programs, subscription identifiers, access tiers, user preferences, or purchase history, enabling account fraud or targeted abuse.</skos:definition>
        <skos:prefLabel>SubscriptionOrMembershipDataLeak</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SudoOrPolicyBypass -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SudoOrPolicyBypass">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Bypassing user privilege enforcement controls such as sudo configurations, SELinux/AppArmor rules, or OS policy restrictions to obtain administrative rights.</skos:definition>
        <skos:prefLabel>SudoOrPolicyBypass</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SuspendThirdPartyAccess -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#SuspendThirdPartyAccess">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Temporarily disabling access for external partners, vendors, or contractors who may be affected by or contributing to a security incident, maintaining isolation until risk is mitigated.</skos:definition>
        <skos:prefLabel>SuspendThirdPartyAccess</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartyAccessRestrictions -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartyAccessRestrictions">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Applying least-privilege controls and conditional access for external partners, vendors, and services to limit the potential blast radius if third-party systems are compromised.</skos:definition>
        <skos:prefLabel>ThirdPartyAccessRestrictions</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartyPatchCompliance -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartyPatchCompliance">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Ensuring external service providers, SaaS platforms, and managed systems follow patching requirements to prevent indirect compromise through the supply chain.</skos:definition>
        <skos:prefLabel>ThirdPartyPatchCompliance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartySecurityPolicy -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ThirdPartySecurityPolicy">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://iee.ihu.gr/intersoc#PolicyandAwareness"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A formal policy defining security requirements, controls, and responsibilities for third-party entities such as vendors, suppliers, service providers, and contractors.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#TrafficVolume -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#TrafficVolume">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A measurement of the data transmitted within a network flow over time, typically represented by packet count or bytes transferred.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#UEFIModification -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#UEFIModification">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Modification to boot firmware or secure boot chains to alter startup flow, disable controls, or load unauthorized components before OS protections activate.</skos:definition>
        <skos:prefLabel>UEFIModification</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateDataIntegrity -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateDataIntegrity">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Confirming that files, databases, and stored artifacts have not been altered, corrupted, encrypted, or tampered with as part of the security incident.</skos:definition>
        <skos:prefLabel>ValidateDataIntegrity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateNetworkConnectivitySafety -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateNetworkConnectivitySafety">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Confirming that restored communication paths do not reintroduce risk or expose systems to unmitigated threats, including firewall rule review, segmentation checks, and secure connectivity validation.</skos:definition>
        <skos:prefLabel>ValidateNetworkConnectivitySafety</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateSecurityControls -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateSecurityControls">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Verifying that protective controls — such as authentication, logging, endpoint protection, segmentation, and alerting — are active and operating effectively after restoration.</skos:definition>
        <skos:prefLabel>ValidateSecurityControls</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateServiceFunctionality -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateServiceFunctionality">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Ensuring business-critical applications and services function correctly post-incident, including availability checks, dependency validation, and performance testing.</skos:definition>
        <skos:prefLabel>ValidateServiceFunctionality</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateSystemIntegrity -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#ValidateSystemIntegrity">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Confirming that the operating system, applications, configurations, firmware, and critical files are in a clean and uncompromised state, free of malware or unauthorized modifications.</skos:definition>
        <skos:prefLabel>ValidateSystemIntegrity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Vishing -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Vishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Exploiting voice communication channels (phone calls, VoIP) to impersonate trusted organizations and extract confidential information.</skos:definition>
        <skos:prefLabel>Vishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#VulnerabilityPrioritizationPolicy -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#VulnerabilityPrioritizationPolicy">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A risk-based framework for ranking remediation according to exploitability, asset criticality, business impact, and threat intelligence to focus patching efforts where most effective.</skos:definition>
        <skos:prefLabel>VulnerabilityPrioritizationPolicy</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#VulnerabilityScanning -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#VulnerabilityScanning">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <owl:backwardCompatibleWith>Continuously scanning systems or applications to identify missing patches, misconfigurations, or exploitable weaknesses before adversaries can target them.</owl:backwardCompatibleWith>
        <skos:definition>Continuously scanning systems or applications to identify missing patches, misconfigurations, or exploitable weaknesses before adversaries can target them.</skos:definition>
        <skos:prefLabel>VulnerabilityScanning</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#WebApplicationRCE -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#WebApplicationRCE">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Achieving remote code execution through vulnerabilities in server-side application logic, frameworks, or interpreters (e.g., unsafe eval, template injection).</skos:definition>
        <skos:prefLabel>WebApplicationRCE</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#WebBrowserExploit -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#WebBrowserExploit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>Exploitation of vulnerabilities in web browsers or browser engines to execute malicious code, access local resources, or escalate privileges—often through drive-by downloads or malicious scripts.</skos:definition>
        <skos:prefLabel>WebBrowserExploit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Whaling -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#Whaling">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://iee.ihu.gr/intersoc#INTERSOC"/>
        <skos:definition>A highly tailored phishing attack focusing on executives or individuals with elevated privileges to gain access to strategic systems or sensitive information.</skos:definition>
        <skos:prefLabel>Whaling</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#isolateHostFromNetwork -->

    <owl:NamedIndividual rdf:about="http://iee.ihu.gr/INTERSOC/ontologies/2025/9/CTI.skos#isolateHostFromNetwork">
