    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Open_Ports_&amp;_Unnecessary_Services -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Open_Ports_&amp;_Unnecessary_Services">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Exposing unprotected services to the internet increases attack surfaces.</skos:definition>
        <skos:example xml:lang="en">Leaving RDP (Remote Desktop Protocol) or SSH open to the public without proper security controls.</skos:example>
        <skos:prefLabel xml:lang="en">Open Ports &amp; Unnecessary Services</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Poor_Identity_&amp;_Access_Management_(IAM) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Poor_Identity_&amp;_Access_Management_(IAM)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Granting excessive privileges increases the risk of unauthorized access.</skos:definition>
        <skos:example xml:lang="en">Giving standard employees administrative privileges, leading to accidental or intentional misuse.</skos:example>
        <skos:prefLabel xml:lang="en">Poor Identity &amp; Access Management (IAM)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Poor_Incident_Response_&amp;_Recovery_Plans -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Poor_Incident_Response_&amp;_Recovery_Plans">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Process_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Lack of preparedness in handling cyber incidents leads to delays and damage escalation.</skos:definition>
        <skos:example xml:lang="en">A company experiencing a ransomware attack without a disaster recovery plan in place.</skos:example>
        <skos:prefLabel xml:lang="en">Poor Incident Response &amp; Recovery Plans</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Privileges_Required_(PR) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Privileges_Required_(PR)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/exploitability_metrics"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">PR</skos:altLabel>
        <skos:definition xml:lang="en">This metric describes the level of privileges an attacker must possess prior to successfully exploiting the vulnerability. The method by which the attacker obtains privileged credentials prior to the attack (e.g., free trial accounts), is outside the scope of this metric. Generally, self-service provisioned accounts do not constitute a privilege requirement if the attacker can grant themselves privileges as part of the attack.</skos:definition>
        <skos:example xml:lang="en">-None.
-Low.
-High.</skos:example>
        <skos:prefLabel xml:lang="en">Privileges Required (PR)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Provider_Urgency_(U) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Provider_Urgency_(U)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/temporal_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">U</skos:altLabel>
        <skos:definition xml:lang="en">Many vendors currently provide supplemental severity ratings to consumers via product security advisories. Other vendors publish Qualitative Severity Ratings from the CVSS Specification Document in their advisories.
To facilitate a standardized method to incorporate additional provider-supplied assessment, an optional pass-through Supplemental Metric called Provider Urgency is available.</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-Red.
-Amber.
-Green.
-Clear.</skos:example>
        <skos:prefLabel xml:lang="en">Provider Urgency (U)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Recovery_(R) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Recovery_(R)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/temporal_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">R</skos:altLabel>
        <skos:definition xml:lang="en">Recovery describes the resilience of a system to recover services, in terms of performance and availability, after an attack has been performed.</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-Automatic.
-User.
-Irrecoverable.</skos:example>
        <skos:prefLabel xml:lang="en">Recovery (R)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Revenge/Punishment -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Revenge/Punishment">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Motivation"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Disgruntled_Employee"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Personal_Revenge"/>
        <skos:definition xml:lang="en">Attacks driven by personal vendettas or perceived injustices.</skos:definition>
        <skos:prefLabel xml:lang="en">Revenge-Punishment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Safety_(S) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Safety_(S)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/temporal_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">S</skos:altLabel>
        <skos:definition xml:lang="en">When a system does have an intended use or fitness of purpose aligned to safety, it is possible that exploiting a vulnerability within that system may have Safety impact which can be represented in the Supplemental Metrics group. Lack of a Safety metric value being supplied does NOT mean that there may not be any Safety-related impacts.</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-Present.
-Negligible.</skos:example>
        <skos:prefLabel xml:lang="en">Safety (S)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Unpatched_Software_&amp;_Operating_Systems -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Unpatched_Software_&amp;_Operating_Systems">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CVE-2017-0144"/>
        <skos:definition xml:lang="en">Failure to update systems leaves them exposed to known security threats.</skos:definition>
        <skos:example xml:lang="en">Running Windows without security updates, making it vulnerable to known exploits like EternalBlue (CVE-2017-0144).</skos:example>
        <skos:prefLabel xml:lang="en">Unpatched Software &amp; Operating Systems</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#User_Interaction_(UI) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#User_Interaction_(UI)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/exploitability_metrics"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">UI</skos:altLabel>
        <skos:definition xml:lang="en">This metric captures the requirement for a human user, other than the attacker, to participate in the successful compromise of the vulnerable system. This metric determines whether the vulnerability can be exploited solely at the will of the attacker, or whether a separate user (or user-initiated process) must participate in some manner.</skos:definition>
        <skos:example xml:lang="en">-None.
-Passive.
-Active.</skos:example>
        <skos:prefLabel xml:lang="en">User Interaction (UI)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Value_Density_(V) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Value_Density_(V)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/temporal_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">V</skos:altLabel>
        <skos:definition xml:lang="en">Value Density describes the resources that the attacker will gain control over with a single exploitation event. It has two possible values, diffuse and concentrated:</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-Diffuse.
-Concentrated.</skos:example>
        <skos:prefLabel xml:lang="en">Value Density (V)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Vulnerability_Response_Effort_(RE) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Vulnerability_Response_Effort_(RE)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/temporal_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">RE</skos:altLabel>
        <skos:definition xml:lang="en">The intention of the Vulnerability Response Effort metric is to provide supplemental information on how difficult it is for consumers to provide an initial response to the impact of vulnerabilities for deployed products and services in their infrastructure. The consumer can then take this additional information on effort required into consideration when applying mitigations and/or scheduling remediation.
When calculating Vulnerability Response Effort, the effort required to deploy the quickest available response should be considered.</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-Low.
-Moderate.
-High.</skos:example>
        <skos:prefLabel xml:lang="en">Vulnerability Response Effort (RE)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#cross_site_request_forgery_(CSRF) -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#cross_site_request_forgery_(CSRF)">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_based"/>
        <skos:definition xml:lang="en">An attack that forces users to perform unintended actions on websites where they are authenticated.</skos:definition>
        <skos:prefLabel xml:lang="en">Cross-Site Request Forgery (CSRF)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/AI-powered_attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/AI-powered_attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/artificial_intelligence"/>
        <skos:definition xml:lang="en">Using artificial intelligence to carry out highly adaptive and sophisticated attacks.</skos:definition>
        <skos:hiddenLabel xml:lang="en">AI</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">AI-Powered Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/AI_in_cybersecurity -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/AI_in_cybersecurity">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/artificial_intelligence"/>
        <skos:definition xml:lang="en">The application of AI and machine learning for threat detection, analysis, and prediction.</skos:definition>
        <skos:hiddenLabel xml:lang="en">AI</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Artificial Intelligence (AI) in Cybersecurity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/API_abuse_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/API_abuse_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Application_Specific_IoCs"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Indicators of unauthorized or excessive API calls.</skos:definition>
        <skos:prefLabel xml:lang="en">API Abuse IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/State-sponsored_APTs"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors"/>
        <skos:altLabel xml:lang="en">APTs</skos:altLabel>
        <skos:definition xml:lang="en">ATP attack is a continuous and targeted cyberattack in which cybercriminals gain access to a network and remain undetected for a significant period.</skos:definition>
        <skos:example xml:lang="en">While countless APT groups operate in the wild, some notable ones have launched numerous large-scale attacks. Some infamous groups include APT28 (Fancy Bear), APT29 (Cozy Bear), APT34 (OilRig, Helix Kitten), and APT38 (Lazarus Group). These are all notable state-sponsored groups funded by various countries.</skos:example>
        <skos:hiddenLabel xml:lang="en">APT</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Advanced Persistent Threats</skos:prefLabel>
        <skos:scopeNote xml:lang="en">APTs employ a large number of tactics to launch attacks. The first methodology used is social engineering, including phishing, spear phishing, smishing, and other targeted tactics to infiltrate a network stealthily. Once they have gained access to the network, APTs work to expand their access to other areas of the network, gaining visibility into the valuable information stored within.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">he goal of APT attacks is to steal sensitive information rather than cause damage to the companys network.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT_patterns_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT_patterns_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Other_Advanced_IoCs"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT"/>
        <skos:definition xml:lang="en">Indicators related to specific threat actor behaviors.</skos:definition>
        <skos:prefLabel xml:lang="en">APT Patterns IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/ARP_spoofing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/ARP_spoofing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spoofing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Manipulating the ARP protocol to intercept data or impersonate devices on a network.</skos:definition>
        <skos:hiddenLabel xml:lang="en">ARP</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">ARP Spoofing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Access_Control -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Access_Control">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Access control measures restrict user access to sensitive data and systems based on roles and permissions. This principle of least privilege minimizes the risk of insider threats and unauthorized access.</skos:definition>
        <skos:prefLabel xml:lang="en">Access Control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ad_blockers -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ad_blockers">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Software that prevents advertisements from appearing with the content the user is intentionally viewing. People block ads for a variety of reasons. For example, many of them find marketing ads annoying and even stressful.</skos:definition>
        <skos:prefLabel xml:lang="en">Ad blockers</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Adware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Adware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Malware that forces unwanted advertisements onto the users device.</skos:definition>
        <skos:example xml:lang="en">Fireball</skos:example>
        <skos:prefLabel xml:lang="en">Adware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Serves unwanted advertisements</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Android -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Android">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:prefLabel xml:lang="en">Google Android OS</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Angler_phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Angler_phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Anglers use fake social media posts to get people to provide login info or download malware.</skos:definition>
        <skos:prefLabel xml:lang="en">Angler Phishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Anti-Government_Hacktivists -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Anti-Government_Hacktivists">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Hacktivists"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Groups targeting governmental agencies for political reasons.</skos:definition>
        <skos:prefLabel xml:lang="en">Anti-Government Hacktivists</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Antimalware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Antimalware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">nti-malware is like a digital security guard for your computer or phone. Its job is to watch out for harmful software (called malware) like viruses, ransomware, or spyware that could sneak in, steal your data, or mess up your system.</skos:definition>
        <skos:example xml:lang="en">If you accidentally download a suspicious file or visit a risky website, anti-malware scans it and blocks anything dangerous before it can cause trouble. Its always on the lookout, protecting your device by detecting and removing threats.</skos:example>
        <skos:prefLabel xml:lang="en">Antimalware</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Antivirus -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Antivirus">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Antivirus is like a shield for your computer or phone, designed to protect it from harmful programs called viruses. Just like your body fights off germs with an immune system, antivirus software detects, blocks, and removes viruses before they can cause damage.</skos:definition>
        <skos:example xml:lang="en">If you accidentally open an infected email or download a sketchy file, antivirus steps in to stop the virus from spreading, stealing your data, or crashing your system.</skos:example>
        <skos:prefLabel xml:lang="en">Antivirus</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Antivirus is your devices digital doctor. It checks for &quot;sick&quot; files and makes sure your system stays healthy.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Application_Layer_Attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Application_Layer_Attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DOS"/>
        <skos:definition xml:lang="en">Application Layer Attacks are a type of cyberattack that targets the application layer (Layer 7) of the OSI (Open Systems Interconnection) model, which handles end-user interaction with software applications. These attacks aim to disrupt or exploit vulnerabilities in specific applications (such as web servers, APIs, or databases) by overwhelming them with requests or exploiting flaws, making the service unavailable to legitimate users.</skos:definition>
        <skos:example xml:lang="en">Target specific applications (e.g., web servers) to exhaust resources like CPU or memory.</skos:example>
        <skos:prefLabel xml:lang="en">Application Layer Attacks</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Application layer attacks are challenging to defend against because they can mimic legitimate user behavior, making it difficult to distinguish between malicious and normal traffic. These attacks are often used in Distributed Denial of Service (DDoS) scenarios.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Application_Specific_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Application_Specific_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Indicators_of_Compromise"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/API_abuse_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/application_crash_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/configuration_changes_IoCs"/>
        <skos:altLabel xml:lang="en">IoCs</skos:altLabel>
        <skos:definition xml:lang="en">IoCs related to specific applications, such as unexpected behaviors, unauthorized modifications, or error messages.</skos:definition>
        <skos:example xml:lang="en">-Web Server Logs: Evidence of attacks such as SQL injection, cross-site scripting (XSS), or directory traversal in log files.
-API Usage Patterns: Unusual or malicious API request patterns, particularly to critical endpoints.
-Cloud Activity: Suspicious activities or access patterns in cloud services, like unauthorized access to data storage buckets.</skos:example>
        <skos:prefLabel xml:lang="en">Application-Specific IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Applications -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Applications">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Systems_and_Technologies"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_Applications"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_Browsers"/>
        <skos:definition xml:lang="en">Applications, or &quot;apps,&quot; are the tools and programs on your devices that help you get things done. They can be as simple as a calculator for quick math or as powerful as video editing software for creating movies. Apps are designed to make tasks easier, faster, or more fun, whether you&apos;re working, learning, or just playing.</skos:definition>
        <skos:prefLabel xml:lang="en">Applications</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Atomic_Indicators -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Atomic_Indicators">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DL"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_Indicators"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Host-Based_Indicators"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Indicators"/>
        <skos:definition xml:lang="en">Short-lived, low-level data points used to identify threats.</skos:definition>
        <skos:prefLabel xml:lang="en">Atomic Indicators</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Attack_Vectors -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Attack_Vectors">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Code_Injection"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Downloads"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_based"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_based"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_based"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/software"/>
        <skos:definition xml:lang="en">Attack Vector is a path or a tool that a threat actor uses to gain access to a system in order to deliver a malicious outcome.
 how to achieve a successful attack?
 e.g. malicious email, attachments, web pages, downloads, deception, hackers, code injection, etc.</skos:definition>
        <skos:prefLabel xml:lang="en">Attack Vectors</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/BAS -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/BAS">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A technology or approach that simulates real-world attack scenarios to test and validate the effectiveness of an organizations defenses.</skos:definition>
        <skos:prefLabel xml:lang="en">Breach and Attack Simulation (BAS)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Backup_and_Recovery -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Backup_and_Recovery">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">Store backups securely, both onsite and offsite, and regularly test recovery procedures.</skos:example>
        <skos:prefLabel xml:lang="en">Backup and Recovery</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Regularly backing up data ensures that critical information can be restored in the event of data loss due to attacks (e.g., ransomware) or disasters.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Bad_Rabbit -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Bad_Rabbit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">The Bad Rabbit malware is embedded into websites using JavaScript injected into the sites HTML code.
If a person clicks on the malicious installer, BadRabbit ransomware encrypts files and presents users with an austere black-and-red message. It reads in part: If you see this text, your files are no longer accessible. You might have been looking for a way to recover your files. Dont waste your time.</skos:example>
        <skos:historyNote xml:lang="en">Bad Rabbit first appeared in 2017 and has similarities to ransomware strains called WannaCry and Petya.</skos:historyNote>
        <skos:prefLabel xml:lang="en">Bad Rabbit</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Bad Rabbit virus locks up victims computers, servers, or files prevents them from regaining access until a ransomusually in Bitcoinis paid.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Baiting -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Baiting">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A form of social engineering where attackers lure victims with promises (e.g., free downloads) to install malware or reveal information.</skos:definition>
        <skos:prefLabel xml:lang="en">Baiting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Behavioral_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Behavioral_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Indicators_of_Compromise"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/process_anomalies_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/system_resource_usage_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/user_activity_IoCs"/>
        <skos:altLabel xml:lang="en">IoCs</skos:altLabel>
        <skos:definition xml:lang="en">Patterns of behavior or activity that suggest malicious intent, such as repeated failed login attempts or unusual network traffic.</skos:definition>
        <skos:example xml:lang="en">-Processes and Services: Specific processes or services spawned by malware.
-Registry Changes: Modifications to the Windows Registry, such as the creation of startup keys.
-Mutexes: Unique identifiers created by malware to prevent multiple instances.
-Scheduled Tasks: Tasks created to enable persistence or execute payloads at specific times.
-Memory Artifacts: Specific memory strings or dumps that indicate malware presence.</skos:example>
        <skos:prefLabel xml:lang="en">Behavioral IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Bots -->

