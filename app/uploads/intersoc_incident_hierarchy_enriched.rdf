        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/delivery_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_Applications"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_Based_IoCs"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_based"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_phishing"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_based"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spear_Phishing"/>
        <skos:definition xml:lang="en">Sending malicious content via email to trick users into opening harmful attachments or clicking on dangerous links.</skos:definition>
        <skos:example xml:lang="en">-Phishing Emails: Deceptive emails that try to steal personal information, like passwords, by pretending to be from a trusted source.
-Spear Phishing: Highly targeted phishing emails tailored to specific individuals or organizations, often using personal details to seem more convincing.
-Malicious Email Attachments: Files attached to emails that, when opened, install harmful software or execute a cyberattack.</skos:example>
        <skos:prefLabel xml:lang="en">Email-based Delivery</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/email_bombing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/email_bombing">
