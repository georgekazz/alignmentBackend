        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/mobile_devices"/>
        <skos:definition xml:lang="en">Taking control of mobile devices through vulnerabilities or malware to steal data or spy on users.</skos:definition>
        <skos:prefLabel xml:lang="en">Mobile Device Hijacking</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/modified_impact_metrics -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/modified_impact_metrics">
