        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Running malicious code in stealthy ways.</skos:definition>
        <skos:example xml:lang="en">-Rundll32 Execution: Using a legitimate Windows tool to run harmful code.
-WMI (Windows Management Instrumentation) Execution: Leveraging Windows tools to execute attacks remotely.</skos:example>
        <skos:prefLabel xml:lang="en">Dynamic Execution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/eavesdropping -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/eavesdropping">
