        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/malware_analysis"/>
        <skos:definition xml:lang="en">A structured format for describing malware attributes and behaviors.</skos:definition>
        <skos:hiddenLabel xml:lang="en">Malware</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">MAEC (Malware Attribute Enumeration and Characterization)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MISP -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MISP">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
