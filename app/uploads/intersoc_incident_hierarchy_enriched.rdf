        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/malware_analysis"/>
        <skos:definition xml:lang="en">An open-source platform for sharing structured threat intelligence between organizations.</skos:definition>
        <skos:hiddenLabel xml:lang="en">Malware</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">MISP (Malware Information Sharing Platform)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MRTI -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MRTI">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/STIX"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TAXII"/>
        <skos:definition xml:lang="en">Threat intelligence provided in a standardized format (e.g., STIX/TAXII) that can be automatically ingested and processed by security systems.</skos:definition>
        <skos:prefLabel xml:lang="en">Machine-Readable Threat Intelligence (MRTI)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malicious_Insider -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malicious_Insider">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Insiders"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Employees or contractors who leak or steal data for personal gain.</skos:definition>
        <skos:prefLabel xml:lang="en">Malicious Insider</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malvertising -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malvertising">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Malvertising is the act of inserting malicious advertisements into otherwise legitimate webpages or advertising networks.</skos:definition>
        <skos:prefLabel xml:lang="en">Malvertising</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware -->

