        <skos:example xml:lang="en">Using &quot;123456,&quot; &quot;password,&quot; or the same password for both email and banking accounts.</skos:example>
        <skos:prefLabel xml:lang="en">Password Reuse &amp; Weak Passwords</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/payload -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/payload">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
