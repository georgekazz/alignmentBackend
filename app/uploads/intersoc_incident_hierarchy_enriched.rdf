        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/evasion_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Devices"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/network_attack_propagation_analysis"/>
        <skos:definition xml:lang="en">Techniques to avoid network monitoring.</skos:definition>
        <skos:example xml:lang="en">-Encrypted Communications: Using encryption to hide malicious traffic.
-Domain Fronting: Masking traffic by routing it through legitimate websites.</skos:example>
        <skos:prefLabel xml:lang="en">Network Evasion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_pivoting -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_pivoting">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
