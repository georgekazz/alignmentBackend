        <skos:definition xml:lang="en">A subscription-based service offered by cybercriminals that enables other attackers to run phishing campaigns with minimal effort.</skos:definition>
        <skos:hiddenLabel xml:lang="en">PhaaS</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Phishing-as-a-Service (PhaaS)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/pharming -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/pharming">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Redirecting users from legitimate websites to fake ones to steal credentials or other information.</skos:definition>
        <skos:prefLabel xml:lang="en">Pharming</skos:prefLabel>
    </owl:NamedIndividual>
    


