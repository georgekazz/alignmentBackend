        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">These metrics enable the consumer analyst to override individual Base metric values based on specific characteristics of a users environment. Characteristics that affect Exploitability or Impact can be reflected via an appropriately modified Environmental metric value.</skos:definition>
        <skos:example xml:lang="en">-Not Defined.
-High.
-Medium.
-Low.</skos:example>
        <skos:prefLabel xml:lang="en">Modified Base Metrics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/multi_factor_authentication -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/multi_factor_authentication">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">MFA</skos:altLabel>
        <skos:definition xml:lang="en">A security system that requires users to provide multiple forms of identification, such as a password and a fingerprint, to gain access.</skos:definition>
        <skos:prefLabel xml:lang="en">Multi-Factor Authentication (MFA)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_access_control -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_access_control">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">NAC</skos:altLabel>
        <skos:definition xml:lang="en">Policies and technologies that enforce security compliance for devices accessing a network.</skos:definition>
        <skos:prefLabel xml:lang="en">Network Access Control (NAC)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_based_exfiltration -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_based_exfiltration">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/data_exfiltration_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Encryption"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Based_IoCs"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Devices"/>
        <skos:definition xml:lang="en">Sending stolen data over the internet.</skos:definition>
        <skos:example xml:lang="en">-Unencrypted File Transfers: Transferring stolen files without any encryption.
-Encrypted Channels (TLS): Using encrypted connections to avoid detection.</skos:example>
        <skos:prefLabel xml:lang="en">Network-Based Exfiltration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_based_movement -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_based_movement">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/lateral_movement_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Based_IoCs"/>
        <skos:definition xml:lang="en">Accessing other devices or systems across a network.</skos:definition>
        <skos:example xml:lang="en">-Exploitation of Remote Services: Exploiting tools like SSH or RDP to gain access.
-SSH Abuse: Using stolen SSH keys to control servers remotely.</skos:example>
        <skos:prefLabel xml:lang="en">Network-Based Movement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/network_evasion -->

