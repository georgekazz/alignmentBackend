        <skos:altLabel xml:lang="en">CVEs</skos:altLabel>
        <skos:definition xml:lang="en">The Common Vulnerabilities and Exposures (CVE) system provides a reference method for publicly known information-security vulnerabilities and exposures.</skos:definition>
        <skos:historyNote xml:lang="en">https://www.cve.org/</skos:historyNote>
        <skos:prefLabel xml:lang="en">Common Vulnerabilities and Exposures</skos:prefLabel>
    </owl:NamedIndividual>
    


