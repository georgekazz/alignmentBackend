        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Unauthorized use of computing resources to mine cryptocurrency.</skos:definition>
        <skos:prefLabel xml:lang="en">Cryptojacking</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_Espionage_Units -->

