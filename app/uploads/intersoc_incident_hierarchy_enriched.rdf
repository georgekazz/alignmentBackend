    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cross-site_scripting_attack -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cross-site_scripting_attack">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
