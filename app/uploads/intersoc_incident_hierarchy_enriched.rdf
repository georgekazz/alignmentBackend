    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fake_updates -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fake_updates">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Tricking users into downloading malicious software disguised as legitimate updates.</skos:definition>
        <skos:prefLabel xml:lang="en">Fake Updates</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/file_droppers -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/file_droppers">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/advanced_malware_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Malware components designed to install additional malicious payloads onto a compromised system.</skos:definition>
        <skos:prefLabel xml:lang="en">File Droppers</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/file_tampering -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/file_tampering">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Vulnerability"/>
        <skos:definition xml:lang="en">Modifying files to alter their intended functionality or introduce vulnerabilities.</skos:definition>
        <skos:prefLabel xml:lang="en">File Tampering</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fileless -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fileless">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Ransomware that resides in-memory and does not leave traditional file traces, making it harder to detect.</skos:definition>
        <skos:prefLabel xml:lang="en">Fileless</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/firmware_attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/firmware_attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Attacks that target a device&apos;s firmware to install low-level malicious code.</skos:definition>
        <skos:prefLabel xml:lang="en">Firmware Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/firmware_rootkit -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/firmware_rootkit">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rootkits"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Infecting the firmware of devices to maintain persistent unauthorized access.</skos:definition>
        <skos:prefLabel xml:lang="en">Firmware Rootkit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/formjacking -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/formjacking">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Injection_attacks"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Injecting malicious code into online forms to steal user-submitted information, such as credit card details.</skos:definition>
        <skos:prefLabel xml:lang="en">Formjacking</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fraud -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fraud">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/post-attack_monetization_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Exploiting stolen information for financial gain.</skos:definition>
        <skos:example xml:lang="en">-Credit Card Abuse: Using stolen credit card details for unauthorized transactions.
-Business Email Compromise (BEC) Profits: Tricking organizations into transferring large sums of money to fraudulent accounts.</skos:example>
        <skos:hiddenLabel xml:lang="en">unauthorized transactions</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Fraud</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/grayware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/grayware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Software that is not explicitly malicious but may still pose risks or exhibit unwanted behavior, such as adware or potentially unwanted programs (PUPs).</skos:definition>
        <skos:prefLabel xml:lang="en">Grayware</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Attack_Vectors"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/hardware_technology"/>
        <skos:definition xml:lang="en">Computer hardware is a collective term used to describe any of the physical components of an analog or digital computer.</skos:definition>
        <skos:prefLabel xml:lang="en">Hardware</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware_exploits -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware_exploits">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/exploitation_procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/hardware"/>
        <skos:definition xml:lang="en">Attacking weaknesses in physical devices.</skos:definition>
        <skos:example xml:lang="en">-Firmware Attacks: Targeting the underlying software controlling hardware devices, like hard drives or routers.
-Side-Channel Attacks: Using indirect clues, like power usage or timing, to extract sensitive data from hardware.</skos:example>
        <skos:prefLabel xml:lang="en">Hardware Exploits</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/homoglyph_attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/homoglyph_attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Exploiting visually similar characters across different scripts to create deceptive domain names or file names.</skos:definition>
        <skos:prefLabel xml:lang="en">Homoglyph Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/homograph_attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/homograph_attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_based"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/homomorphic_encryption"/>
        <skos:definition xml:lang="en">Using visually similar characters (e.g., &quot;rn&quot; instead of &quot;m&quot;) in domain names to trick users into visiting malicious sites.</skos:definition>
        <skos:prefLabel xml:lang="en">Homograph Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/honeypot -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/honeypot">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A decoy system or network set up to attract and monitor attackers, helping organizations gather intelligence on their tactics and techniques.</skos:definition>
        <skos:prefLabel xml:lang="en">Honeypot</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/iOS -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/iOS">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:prefLabel xml:lang="en">Apple iOS</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/image_based_attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/image_based_attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Embedding malicious code in image files to execute payloads when the images are viewed.</skos:definition>
        <skos:hiddenLabel xml:lang="en">payload</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Image-Based Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/tactics"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">It defines the overall impact or harm caused to the victims.</skos:definition>
        <skos:hiddenLabel xml:lang="en">identity theft</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Impact</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Impact includes monetary theft, identity theft, defamation and
cyberbullying.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact_metrics -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact_metrics">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/base_score"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">The Impact metrics capture the effects of a successfully exploited vulnerability. Analysts should constrain impacts to a reasonable, final outcome which they are confident an attacker is able to achieve.</skos:definition>
        <skos:example xml:lang="en">-Confidentiality Impact (C): None, Low, or High.
-Integrity Impact (I): None, Low, or High.
-Availability Impact (A): None, Low, or High.</skos:example>
        <skos:prefLabel xml:lang="en">Impact Metrics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact_procedures -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/impact_procedures">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/procedures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/data_destruction"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/ransom_activities"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/service_disruption"/>
        <skos:definition xml:lang="en">How attackers cause harm or achieve their goals.</skos:definition>
        <skos:hiddenLabel xml:lang="en">harm</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Impact Procedures</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/infinite_loop_exploits -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/infinite_loop_exploits">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Introducing malicious code that causes systems to crash or become unresponsive by creating infinite loops.</skos:definition>
        <skos:hiddenLabel xml:lang="en">malicious code</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Infinite Loop Exploits</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/initial_access -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/initial_access">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/tactics"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">The first phase in an attack where the threat actor gains entry to a target&apos;s systems (e.g., through phishing, exploits, or credential theft).</skos:definition>
        <skos:prefLabel xml:lang="en">Initial Access</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/initial_contact -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/initial_contact">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/tactics"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">During this tactic, criminals attempt to connect with the victim or victim object for the first time.</skos:definition>
        <skos:hiddenLabel xml:lang="en">phishing</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Initial Contact</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Initial Contact can be made in a variety of ways, including through phishing
calls or SMS, fake websites, or social media messages.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/integrity_requirement -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/integrity_requirement">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/security_requirements"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:prefLabel xml:lang="en">Integrity Requirement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/island_hopping -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/island_hopping">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">An attack technique where adversaries compromise smaller, less secure organizations to pivot into larger targets.</skos:definition>
        <skos:prefLabel xml:lang="en">Island Hopping</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/lack_of_awareness_about_cyber_threats -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/lack_of_awareness_about_cyber_threats">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Human_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Users are often unaware of common cybersecurity risks and best practices.</skos:definition>
        <skos:example xml:lang="en">Opening email attachments from unknown senders or connecting to unsecured public Wi-Fi networks.</skos:example>
        <skos:prefLabel xml:lang="en">Lack of Awareness about Cyber Threats</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/lack_of_two_factor_authentication -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/lack_of_two_factor_authentication">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Human_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Not enabling 2FA makes accounts much easier to compromise.</skos:definition>
