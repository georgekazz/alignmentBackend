        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:definition xml:lang="en">Links to fake websites or credentials harvesting.</skos:definition>
        <skos:prefLabel xml:lang="en">Phishing IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phishing_kit -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phishing_kit">
