        <skos:prefLabel xml:lang="en">Critical Infrastructure Sector</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cryptojacking -->

