    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/credential_arvesting -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/credential_arvesting">
