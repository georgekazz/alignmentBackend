        <skos:example xml:lang="en">Astaroth</skos:example>
        <skos:hiddenLabel xml:lang="en">Malware</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Fileless Malware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Makes changes to files that are native to the OS</skos:scopeNote>
    </owl:NamedIndividual>
    


