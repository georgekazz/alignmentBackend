        <skos:definition xml:lang="en">Flooding an email inbox with excessive messages to overwhelm the recipient.</skos:definition>
        <skos:prefLabel xml:lang="en">Email Bombing</skos:prefLabel>
    </owl:NamedIndividual>
    


