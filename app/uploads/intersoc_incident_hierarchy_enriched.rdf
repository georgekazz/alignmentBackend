        <skos:prefLabel xml:lang="en">Corporate Watchdog Hacktivists</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Critical_Infrastructure_Sector -->

