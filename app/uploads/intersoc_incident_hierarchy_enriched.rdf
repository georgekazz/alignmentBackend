        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/VPN"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cryptography"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cyber_threat_fusion_center"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/deception_grid"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/decryption"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/digital_certificate"/>
