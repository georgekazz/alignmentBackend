        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/credential_manipulation"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/system_modifications"/>
        <skos:definition xml:lang="en">How attackers maintain long-term access to systems.</skos:definition>
        <skos:hiddenLabel xml:lang="en">long access</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Persistence Procedures</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phaaS -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phaaS">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
