        <skos:prefLabel xml:lang="en">Credential Harvesting</skos:prefLabel>
        <skos:scopeNote xml:lang="en">It could be accomplished using a keylogger or remote access tool, phishing with the assistance of social engineering, emotional exploitation in a romantic relationship, credential stuffing, or brute force.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/credential_based_movement -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/credential_based_movement">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
