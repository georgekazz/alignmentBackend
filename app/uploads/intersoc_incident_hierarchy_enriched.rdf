        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Adware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cryptojacking"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Keyloggers"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malvertising"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/RAT"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rootkits"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spyware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Trojans"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Viruses"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Worms"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/backdoor"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/disk_wiping"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/grayware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/image_based_attacks"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/logic_bomb"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/payload"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/polymorphic_malware"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/rogue_software_updates"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/steganography"/>
        <skos:relatedMatch rdf:resource="http://data.jrc.ec.europa.eu/ontology/cybersecurity/malware_analysis"/>
        <skos:relatedMatch rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/watermarking"/>
        <skos:definition xml:lang="en">Malware (short for malicious software) refers to any software specifically designed to disrupt, damage, or gain unauthorized access to computer systems, networks, or devices. Its primary objective is to harm or exploit the user and their data.</skos:definition>
        <skos:prefLabel xml:lang="en">Malware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Malware is often distributed through infected email attachments, malicious websites, software downloads, or even social engineering attacks. Its impact ranges from minor annoyances (e.g., displaying ads) to severe breaches of security (e.g., stealing sensitive personal or financial information).en</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Manufacturing_and_Industrial_Firms -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Manufacturing_and_Industrial_Firms">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Companies"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Companies producing goods, often targeted for supply chain attacks.</skos:definition>
        <skos:prefLabel xml:lang="en">Manufacturing and Industrial Firms</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MiTM -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MiTM">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">MiTM</skos:altLabel>
        <skos:definition xml:lang="en">With a man-in-the-middle attack, the hacker gets in the middle of two parties and tries to steal information exchanged between them, such as account credentials.</skos:definition>
        <skos:hiddenLabel xml:lang="en">Man-in-the-middle</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Man-in-the-middle (MiTM)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Motivation -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Motivation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Motivation"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_Warfare"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Espionage"/>
        <skos:definition xml:lang="en">Cyber operations conducted by or against military entities for strategic advantage.</skos:definition>
        <skos:prefLabel xml:lang="en">Military Motivation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Systems -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Systems">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nations"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Defense communication and intelligence networks.</skos:definition>
        <skos:prefLabel xml:lang="en">Military Systems</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Mobile_Malware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Mobile_Malware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">Triada</skos:example>
        <skos:hiddenLabel xml:lang="en">Triada</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">Mobile Malware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Infects mobile devices</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Motivation -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Motivation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DL"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Revenge/Punishment"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Financial_Motivation"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ideological_Motivation"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Motivation"/>
        <skos:definition xml:lang="en">The driving force behind a threat actors actions, often related to ideological, military, financial, or personal objectives.</skos:definition>
        <skos:prefLabel xml:lang="en">Motivation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nation_States -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nation_States">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Nation states and governments frequently fund threat actors with the goal of stealing sensitive data, gathering confidential information or disrupting another governments critical infrastructure. These malicious activities often include espionage or cyberwarfare and tend to be highly funded, making the threats complex and challenging to detect.</skos:definition>
        <skos:prefLabel xml:lang="en">Nation States</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nations -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nations">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Economic_Institutions"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Election_Systems"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Government_Networks"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Military_Systems"/>
        <skos:definition xml:lang="en">Countries targeted for geopolitical, military, or economic reasons.</skos:definition>
        <skos:prefLabel xml:lang="en">Nations</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Based_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Based_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Indicators_of_Compromise"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DNS_anomalies_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/malicious_ip_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/unusual_traffic_patterns_IoCs"/>
        <skos:altLabel xml:lang="en">IoCs</skos:altLabel>
        <skos:definition xml:lang="en">Indicators related to network activity, such as abnormal IP addresses, data transfers, or unusual communication patterns.</skos:definition>
        <skos:example xml:lang="en">-IP Addresses: Malicious or suspicious IP addresses known to be involved in attacks.
-Domains: Suspicious or known-malicious domains or URLs used for C2 (command and control).
-URL Patterns: Common URL structures used in phishing or malware distribution.
-Domain Name System (DNS) Artifacts: Suspicious DNS queries, domain generation algorithms (DGAs), or atypical DNS records.
-Network Traffic Patterns: Specific patterns in packet data or protocols, such as unusual ports, HTTP headers, or abnormal packet sizes.</skos:example>
        <skos:prefLabel xml:lang="en">Network-Based IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Devices -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Devices">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Systems_and_Technologies"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">Routers, Firewalls, Switches, IDS/IPS, etc.</skos:example>
        <skos:prefLabel xml:lang="en">Network Devices</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Indicators -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Indicators">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Atomic_Indicators"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">IP Addresses  Public or private IPs linked to cyber threats.
Domain Names  Malicious domains used in phishing or malware campaigns.</skos:example>
        <skos:prefLabel xml:lang="en">Network Indicators</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Segmentation -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Segmentation">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">Use VLANs, firewalls, and access controls to separate different parts of the network.</skos:example>
        <skos:prefLabel xml:lang="en">Network Segmentation</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Dividing a network into segments limits the spread of malware and contains potential breaches. It helps control access to sensitive information and reduces the attack surface.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_based -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_based">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Attack_Vectors"/>
        <skos:definition xml:lang="en">Files or documents that are sent along with an email or message. In cybersecurity, attachments are a common attack vector used by cybercriminals to deliver malware, phishing content, or malicious scripts to a recipient. When opened, these files may exploit vulnerabilities or trick the user into executing harmful actions. Organizations often use filtering and scanning tools to detect and block suspicious attachments to protect against potential threats.</skos:definition>
        <skos:prefLabel xml:lang="en">Attachments</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/No_Regular_Security_Audits_or_Assessments -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/No_Regular_Security_Audits_or_Assessments">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Process_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Failure to regularly test security controls results in undetected vulnerabilities.</skos:definition>
        <skos:example xml:lang="en">A company never conducting penetration tests, leaving security gaps unaddressed.</skos:example>
        <skos:prefLabel xml:lang="en">No Regular Security Audits or Assessments</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Non-Governmental_Organizations -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Non-Governmental_Organizations">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Organizations"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">NGOs</skos:altLabel>
        <skos:definition xml:lang="en">Humanitarian, environmental, or political advocacy groups.</skos:definition>
        <skos:prefLabel xml:lang="en">Non-Governmental Organizations (NGOs)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/OSINT -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/OSINT">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Information gathered from publicly available sources, such as websites, social media, and news outlets, used for intelligence purposes.</skos:definition>
        <skos:prefLabel xml:lang="en">Open Source Intelligence (OSINT)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Offensive_Cyber_Warfare_Groups -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Offensive_Cyber_Warfare_Groups">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nation_States"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Actors that engage in cyberattacks to disable enemy infrastructure.</skos:definition>
        <skos:prefLabel xml:lang="en">Offensive Cyber Warfare Groups</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Systems_and_Technologies"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Android"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Linux"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Windows"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/iOS"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/macOS"/>
        <skos:definition xml:lang="en">An operating system (OS) is the basic software that makes your computer, phone, or other devices work. Its like the manager of a busy office, making sure everything runs smoothly. It helps different parts of your devicelike the hardware, applications, and userscommunicate and work together efficiently.</skos:definition>
        <skos:example xml:lang="en">Windows, Linux, macOS, Android, iOS, etc.</skos:example>
        <skos:prefLabel xml:lang="en">Operating Systems</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Organizations -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Organizations">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Educational_Institutions"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Government_Agencies"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Healthcare_Organizations"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Non-Governmental_Organizations"/>
        <skos:definition xml:lang="en">Public or private entities targeted for cyberattacks, often due to their assets, influence, or vulnerabilities.</skos:definition>
        <skos:prefLabel xml:lang="en">Organizations</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Other_Advanced_IoCs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Other_Advanced_IoCs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Indicators_of_Compromise"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT_patterns_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/combined_signals_IoCs"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cross_domain_IoCs"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/YARA"/>
        <skos:altLabel xml:lang="en">IoCs</skos:altLabel>
        <skos:definition xml:lang="en">Complex or multifaceted IoCs that involve correlations between multiple data sources, such as patterns indicating advanced persistent threats (APT).</skos:definition>
        <skos:example xml:lang="en">-Digital Certificates: Malicious or compromised certificates used to sign malware.
-Machine Learning-Based IoCs: Behaviors and anomalies detected through machine learning that indicate compromise.
-YARA Rules: Custom patterns used to match malware signatures in files or memory.</skos:example>
        <skos:prefLabel xml:lang="en">Other Advanced IoCs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Outdated_or_Unsupported_Hardware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Outdated_or_Unsupported_Hardware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems"/>
        <skos:definition xml:lang="en">Legacy systems often lack security updates and become easy targets for attackers.</skos:definition>
        <skos:example xml:lang="en">Running Windows XP or outdated Linux distributions with unpatched vulnerabilities.</skos:example>
        <skos:prefLabel xml:lang="en">Outdated or Unsupported Hardware</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Password_attack -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Password_attack">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/password_cracking"/>
        <skos:definition xml:lang="en">As the name suggests, these attacks involve cybercriminals trying to guess or steal the password or login credentials to a users account. Many password attacks use social engineering to trick victims into unwittingly sharing this sensitive data. However, hackers can also use brute force attacks to steal passwords, repeatedly trying different password combinations until one is successful.</skos:definition>
        <skos:prefLabel xml:lang="en">Password Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Personal_Revenge -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Personal_Revenge">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos#Revenge/Punishment"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Attacks targeting individuals.</skos:definition>
        <skos:prefLabel xml:lang="en">Personal Revenge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pharming -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pharming">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">In a pharming attack, the victim gets malicious code installed on their computer. This code then sends the victim to a fake website designed to gather their login credentials.</skos:definition>
        <skos:prefLabel xml:lang="en">Pharming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Social_Engineering"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Angler_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Baiting"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Clone_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Deceptive_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Domain_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Evil_twin_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/HTTPS_Phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Image_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pharming"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pop_up_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pretexting"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Search_engine_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Smishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spear_Phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Vishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Watering_hole_phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Website_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Whaling"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phaaS"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/phishing_kit"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/QR_code_phishing"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_based"/>
        <skos:definition xml:lang="en">Phishing is a type of cyberattack where an attacker attempts to deceive individuals into revealing sensitive information, such as usernames, passwords, credit card numbers, or other personal data. This is typically done by masquerading as a trustworthy entity in digital communication, such as an email, message, or website.</skos:definition>
        <skos:historyNote xml:lang="en">Phishing is one of the most common forms of cyberattack, and it can lead to identity theft, financial loss, and the compromise of personal or corporate data.</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Political_Activism -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Political_Activism">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ideological_Motivation"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Cyber operations to promote political causes.</skos:definition>
        <skos:prefLabel xml:lang="en">Political Activism</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Poor_Data_Encryption_Practices -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Poor_Data_Encryption_Practices">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Process_Vulnerabilities"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Storing or transmitting sensitive data without encryption increases security risks.</skos:definition>
        <skos:example xml:lang="en">Customer credit card details stored in plain text instead of being encrypted.</skos:example>
        <skos:prefLabel xml:lang="en">Poor Data Encryption Practices</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pop_up_phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pop_up_phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Pop-up phishing often uses a pop-up about a problem with your computers security or some other issue to trick you into clicking. You are then directed to download a file, which ends up being malware, or to call what is supposed to be a support center.</skos:definition>
        <skos:prefLabel xml:lang="en">Pop-up Phishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pretexting -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Pretexting">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Creating a fabricated scenario to trick a target into providing sensitive information, often impersonating trusted entities.</skos:definition>
        <skos:prefLabel xml:lang="en">Pretexting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Process_Vulnerabilities -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Process_Vulnerabilities">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Vulnerability"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Process vulnerabilities are weaknesses in the way organizations manage their operations, workflows, or decision-making that can lead to security breaches. These aren&apos;t flaws in the technology itself but in the procedures and steps people follow (or fail to follow) to keep systems and data secure.</skos:definition>
        <skos:example xml:lang="en">Imagine a company has a policy for updating software but no clear schedule or accountability for who does it. If updates are missed, this gap could leave systems open to attack.</skos:example>
        <skos:prefLabel xml:lang="en">Process Vulnerabilities</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/QR_code_phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/QR_code_phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Web_based"/>
        <skos:definition xml:lang="en">Embedding malicious URLs in QR codes to trick users into visiting harmful websites.</skos:definition>
        <skos:hiddenLabel xml:lang="en">malicious URL</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">QR Code Phishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/RAT -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/RAT">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <usesMethod rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Downloads"/>
        <usesMethod rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Email_based"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">RAT</skos:altLabel>
        <skos:definition xml:lang="en">A Remote Access Trojan (RAT) is a type of malware that allows an attacker to gain unauthorized remote control over a victim&apos;s computer or device. Once installed, the RAT provides the attacker with a wide range of functionalities, enabling them to monitor the victim&apos;s activities, steal sensitive information, and manipulate the system without the user&apos;s knowledge.</skos:definition>
        <skos:prefLabel xml:lang="en">Remote Access Trojan (RAT)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/RFID_skimming -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/RFID_skimming">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Intercepting data from RFID-enabled devices like credit cards or access badges.</skos:definition>
        <skos:hiddenLabel xml:lang="en">RFID</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">RFID Skimming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Bad_Rabbit"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cerber"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/fileless"/>
        <skos:definition xml:lang="en">Ransomware is a type of malicious software (malware) designed to encrypt a victims data or lock them out of their computer systems, rendering the data inaccessible. The attacker then demands a ransom, typically in cryptocurrency, in exchange for restoring access to the encrypted files or systems. If the ransom is not paid, the attacker may threaten to permanently block access to the data, delete it, or leak sensitive information.</skos:definition>
        <skos:example xml:lang="en">RYUK</skos:example>
        <skos:prefLabel xml:lang="en">Ransomware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Disables victim&apos;s access to data until ransom is paid</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware_Extortion -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware_Extortion">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Financial_Motivation"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware"/>
        <skos:definition xml:lang="en">Encrypting data and demanding ransom for decryption keys.</skos:definition>
        <skos:prefLabel xml:lang="en">Ransomware Extortion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware_Syndicates -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware_Syndicates">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_criminals"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ransomware"/>
        <skos:definition xml:lang="en">Groups that deploy ransomware to extort victims.</skos:definition>
        <skos:prefLabel xml:lang="en">Ransomware Syndicates</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Regular_Software_Updates -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Regular_Software_Updates">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Keeping operating systems, applications, and security software up to date helps fix vulnerabilities that attackers could exploit.</skos:definition>
        <skos:example xml:lang="en">mplement a patch management strategy to ensure timely application of updates.</skos:example>
        <skos:prefLabel xml:lang="en">Regular Software Updates and Patch Management</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Religious_Extremism -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Religious_Extremism">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Ideological_Motivation"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Cyberattacks motivated by religious beliefs.</skos:definition>
        <skos:prefLabel xml:lang="en">Religious Extremism</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Resilience -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Resilience">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">The ability of an organisation to manage cybersecurity incidents, recover from failure or damage and keep running continuously despite growing threats.</skos:definition>
        <skos:prefLabel xml:lang="en">Resilience</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Retail_and_E-commerce_Sector -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Retail_and_E-commerce_Sector">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Sectors"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Businesses handling online transactions and customer data.</skos:definition>
        <skos:prefLabel xml:lang="en">Retail and E-commerce Sector</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rogue_wireless_device -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rogue_wireless_device">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/rogue_access_points"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Unauthorised hardware that is connected to or near an organisations wireless network. The device can be used to gain access to sensitive data, send it back to an adversary or connect other devices to a network.</skos:definition>
        <skos:prefLabel xml:lang="en">Rogue wireless device</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rootkits -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Rootkits">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/firmware_rootkit"/>
        <skos:definition xml:lang="en">A rootkit is a type of malicious software which is executed before your computer&apos;s operating system has completed boot up - each time your computer boots up the rootkit is executed.</skos:definition>
        <skos:example xml:lang="en">Zacinlo</skos:example>
        <skos:prefLabel xml:lang="en">Rootkits</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Software is injected into applications, firmware, operating system kernels or hypervisors, providing remote administrative access to a computer. The attacker can start the operating system within a compromised environment, gain complete control of the computer and deliver additional malware.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SIEMs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SIEMs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A solution that collects and analyzes security events from across an organizations infrastructure to detect and respond to threats.</skos:definition>
        <skos:example xml:lang="en">-Splunk
-QRadar
-Elastic Stack</skos:example>
        <skos:prefLabel xml:lang="en">SIEMs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SIM_swapping -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SIM_swapping">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Hijacking phone numbers by exploiting mobile carrier vulnerabilities to intercept calls and messages.</skos:definition>
        <skos:prefLabel xml:lang="en">SIM Swapping</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SOAR -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SOAR">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A set of technologies that enable organizations to automate security processes, coordinate threat response, and improve efficiency in handling incidents.</skos:definition>
        <skos:prefLabel xml:lang="en">Security Orchestration, Automation, and Response (SOAR)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SOCKS -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SOCKS">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">SOCKS</skos:altLabel>
        <skos:definition xml:lang="en">SOCKS s an internet protocol that exchanges network packets between a client and server through a proxy server.</skos:definition>
        <skos:prefLabel xml:lang="en">Socket Secure</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/STIX -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/STIX">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TAXII"/>
        <skos:definition xml:lang="en">A standardized language for representing cyber threat intelligence, enabling the sharing of structured and machine-readable data.</skos:definition>
        <skos:prefLabel xml:lang="en">STIX (Structured Threat Information eXpression)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SYN_flood -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/SYN_flood">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DOS"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A type of DoS attack that overwhelms a target with incomplete TCP connection requests.</skos:definition>
        <skos:hiddenLabel xml:lang="en">DoS</skos:hiddenLabel>
        <skos:prefLabel xml:lang="en">SYN Flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Script_Kiddies -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Script_Kiddies">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Script Kiddies are unskilled individuals who use pre-made scripts, tools, or exploits developed by others to perform cyberattacks. They lack the technical expertise to create or understand the underlying code but can still cause significant damage by leveraging these ready-made resources.</skos:definition>
        <skos:example xml:lang="en">-Defacing websites.
-Launching DDoS attacks using tools like LOIC (Low Orbit Ion Cannon).
-Exploiting basic vulnerabilities in systems or applications.</skos:example>
        <skos:prefLabel xml:lang="en">Script Kiddies</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Search_engine_phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Search_engine_phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">A search engine phishing attack involves an attacker making fake products that look attractive. When these pop up in a search engine, the target is asked to enter sensitive information before purchasing, which then goes to a hacker.</skos:definition>
        <skos:prefLabel xml:lang="en">Search Engine Phishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Sectors -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Sectors">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Critical_Infrastructure_Sector"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Finance_and_Banking_Sector"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Healthcare_and_Biotechnology_Sector"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Retail_and_E-commerce_Sector"/>
        <skos:definition xml:lang="en">Broad industries that face specific cybersecurity threats based on their operations.</skos:definition>
        <skos:prefLabel xml:lang="en">Sectors</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Security_Awareness_Training -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Security_Awareness_Training">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Defensive_Measures"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:example xml:lang="en">Conduct regular training sessions and simulated phishing exercises.</skos:example>
        <skos:prefLabel xml:lang="en">Security Awareness Training</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Educating employees about cybersecurity best practices, phishing tactics, and safe online behavior helps reduce human error, which is a significant factor in security breaches.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Side_channel_attack -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Side_channel_attack">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Any attack that leverages information gained from a system&apos;s operation, such as power consumption, sound, computation timings or electromagnetic leaks, instead of from vulnerabilities in the system itself.</skos:definition>
        <skos:prefLabel xml:lang="en">Side-channel Attacks</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Smishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Smishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Phishing carried out through SMS messages, tricking users into revealing sensitive data or installing malware.</skos:definition>
        <skos:prefLabel xml:lang="en">Smishing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Social_Engineering -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Social_Engineering">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/typosquatting"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/typosquatting_variants"/>
        <skos:definition xml:lang="en">Social engineering attacks manipulate people into sharing information that they shouldnt share, downloading software that they shouldnt download, visiting websites they shouldnt visit, sending money to criminals or making other mistakes that compromise their personal or organizational security.</skos:definition>
        <skos:prefLabel xml:lang="en">Social Engineering</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Social_Justice_Hacktivists -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Social_Justice_Hacktivists">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Hacktivists"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Cyber actors advocating for human rights, environment, or minority groups.</skos:definition>
        <skos:prefLabel xml:lang="en">Social Justice Hacktivists</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spear_Phishing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spear_Phishing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Phishing"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Spear phishing involves targeting a specific individual in an organization to try to steal their login credentials. The attacker often first gathers information about the person before starting the attack, such as their name, position, and contact details.</skos:definition>
        <skos:prefLabel xml:lang="en">Spear Phising</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spoofing -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spoofing">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/ARP_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DNS_spoofing_poisoning"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Geo_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/MAC_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/content_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/email_spoofing"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/voice_spoofing"/>
        <skos:altLabel xml:lang="en">Spoofing</skos:altLabel>
        <skos:definition xml:lang="en">An attacker or program successfully masquerades as another by falsifying data for malicious reasons. Spoofing an email address to fool a recipients or an attacker spoofing their IP or hardware (mac) address in a man-in-the-middle attack are well known attack examples.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spyware -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Spyware">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Malware"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Software that secretly monitors and collects user activity and data.</skos:definition>
        <skos:example xml:lang="en">DarkHotel</skos:example>
        <skos:prefLabel xml:lang="en">Spyware</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Collects user activity data without their knowledge</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/State-sponsored_APTs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/State-sponsored_APTs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/APT"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nation_States"/>
        <skos:definition xml:lang="en">APT attacks are planned and executed by well-funded nation-state cybercriminal groups.</skos:definition>
        <skos:prefLabel xml:lang="en">State Sponsored APTs</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Supply_Chain_Attacks -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Supply_Chain_Attacks">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Supply chain attacks are a new type of threat to software developers and vendors. Its purpose is to infect legitimate applications and distribute malware via source code, build processes or software update mechanisms.</skos:definition>
        <skos:prefLabel xml:lang="en">Supply Chain Attacks</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Attackers are looking for non-secure network protocols, server infrastructure, and coding techniques, and use them to compromise build and update process, modify source code and hide malicious content.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/System_Vulnerabilities">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Vulnerability"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">System vulnerabilities are weaknesses or flaws in software, hardware, or network configurations that can be exploited by attackers to compromise a systems security. These vulnerabilities are like unlocked doors or windows in a house  theyre not inherently harmful on their own but can be used by intruders to gain unauthorized access or cause damage.</skos:definition>
        <skos:example xml:lang="en">If a computer program has outdated security patches, an attacker could exploit that gap to install malware or steal sensitive data.</skos:example>
        <skos:prefLabel xml:lang="en">System Vulnerabilities</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Systems_and_Technologies -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Systems_and_Technologies">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Applications"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Network_Devices"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Operating_systems"/>
        <skos:prefLabel xml:lang="en">Systems and Technologies</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TAXII -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TAXII">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/STIX"/>
        <skos:definition xml:lang="en">A protocol for securely sharing threat intelligence over networks, often used in conjunction with STIX.</skos:definition>
        <skos:prefLabel xml:lang="en">TAXII (Trusted Automated eXchange of Indicator Information)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TIPs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TIPs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/cybersecurity_tools_and_techniques"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:altLabel xml:lang="en">TIPs</skos:altLabel>
        <skos:definition xml:lang="en">A software solution designed to collect, analyze, and manage threat intelligence data to help organizations make informed security decisions.</skos:definition>
        <skos:example xml:lang="en">-ThreatConnect
-Recorded Future
-Anomali.</skos:example>
        <skos:prefLabel xml:lang="en">Threat Intelligence Platforms (TIPs)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TLS_downgrade -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TLS_downgrade">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:related rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Encryption"/>
        <skos:definition xml:lang="en">Forcing systems to use weaker encryption protocols, making it easier to intercept or manipulate communications.</skos:definition>
        <skos:prefLabel xml:lang="en">TLS Downgrade</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TTPs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/TTPs">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DL"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Types"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/procedures"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/tactics"/>
        <skos:altLabel xml:lang="en">TTP</skos:altLabel>
        <skos:definition xml:lang="en">TTPs refer to the patterns and methods attackers use. Tactics are high-level objectives (e.g., gaining access), techniques are specific ways to achieve tactics (e.g., phishing), and procedures are the technical details of execution. TTPs help cybersecurity teams anticipate attacker moves.</skos:definition>
        <skos:prefLabel xml:lang="en">Tactics, Techniques, and Procedures (TTPs)</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Target">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DL"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Companies"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Individuals"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nations"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Organizations"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Sectors"/>
        <skos:definition xml:lang="en">The intended victim or affected entity of a cyberattack. Targets can include organizations, companies, sectors, nations, and individuals.</skos:definition>
        <skos:prefLabel xml:lang="en">Target</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Tech_Companies -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Tech_Companies">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Companies"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">Organizations developing software, cloud services, and digital products.</skos:definition>
        <skos:prefLabel xml:lang="en">Tech Companies</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Terrorists -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Terrorists">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:definition xml:lang="en">People who uses unlawful violence and intimidation, especially against civilians, in the pursuit of political aims.</skos:definition>
        <skos:prefLabel xml:lang="en">Terrorists</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Threat_Actors">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/DL"/>
        <skos:inScheme rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/CYBERSECURITY_GLOSSARY"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_Fighters"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_Terrorists"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Cyber_criminals"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Hackers"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Hacktivists"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Insiders"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Nation_States"/>
        <skos:narrower rdf:resource="http://www.semanticweb.org/INTERSOC/ontologies/2024/9/CTI.skos/Script_Kiddies"/>
